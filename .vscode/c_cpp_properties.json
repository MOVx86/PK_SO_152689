{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/src/**",
                "/usr/include/**",
                "/usr/local/include/**",
                "/usr/lib/gcc/x86_64-linux-gnu/13/include/**"
            ],
            "defines": [
                "_DEBUG"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++17",
            "intelliSenseMode": "linux-gcc-x64",
            "compilerArgs": [
                "-std=gnu17"
            ],
            "browse": {
                "path": [
                    "/usr/include/**"
                ],
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            }
        }
    ],
    "version": 4
}