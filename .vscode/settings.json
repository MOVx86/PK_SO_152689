{
    "todohighlight.isEnable": true,
    "todohighlight.isCaseSensitive": true,
    "todohighlight.keywords": [
        {
            "text": "TODO",
            "color": "#000000",
            "backgroundColor": "#FDFD96",
        },
        {
            "text": "DOC-MISSING",
            "color": "#FFFFFF",
            "backgroundColor": "#FF6961"
        },
        {
            "text": "DOC-READY",
            "color": "#FFFFFF",
            "backgroundColor": "#82A65E"
        },
        {
            "text": "UNFINISHED",
            "color": "#FFFFFF",
            "backgroundColor": "#FF6961"
        },
        {
            "text": "FINISHED",
            "color": "#FFFFFF",
            "backgroundColor": "#82A65E"            
        }
    ],

    "markdownlint.config": {
        "MD033": false
    },

    "C_Cpp.default.compilerPath": "/usr/bin/gcc",
    "files.associations": {
        "ipc_utils.h": "c",
        "manager.h": "c",
        "pid_array.h": "c"
    }
}