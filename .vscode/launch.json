{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DEBUG GCC Launch - WSL",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/bin/debug_app",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty print",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": false
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb",
            "preLaunchTask": "(DEBUG) build",
            "postDebugTask": ""
        }
    ]
}